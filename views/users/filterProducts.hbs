 <div class="col-10 mt-5">

            <div class=" ">
                <div class="row property__gallery">
                    {{#each products}}
                    <div class="col-sm-6 col-md-2 ">
                        <div class="product-grid  ">
                            <div class="product-image men">
                                <a href="/product/details/{{this.products._id}}" class="image ">
                                    <img src="/product-images/{{this.products._id}}(1).jpg" class="rounded-lg mt-3">
                                </a>
                                <ul class="product-links">
                                    {{!-- <li><a href="#"><i class="fas fa-shopping-cart"></i></a></li> --}}
                                    {{!-- <li> <input type="button"><i class="fa fa-heart"></i></a></li> --}}
                                    <li> <button onclick="addToWishlist('{{this.products._id}}')" class="btn ">
                                            {{!-- <i class="fa fa-heart"></i> --}}
                                            <i class="bi bi-heart"></i>
                                        </button></li>

                                    {{!-- <li><a href="#"><i class="fa fa-random"></i></a></li> --}}
                                </ul>
                                {{!-- <a href="" class="add-to-cart ">Add to Cart</a> --}}
                            </div>
                            <div class=" ">
                                <h6 class="  fs-6 fw-bold p-0"><a class="text-dark"
                                        href="#">{{this.products.brandName}}</a></h6>
                                <br>
                                <div>
                                    <p class=""><a class="text-dark m-0 " href="#">{{this.products.title}}</a></p>

                                </div>
                                <div class="price ">Rs.{{this.products.price}}</div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>